<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>Filter_UTF-8</filter-name>
        <filter-class>Filter.Filter_UTF8</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Filter_UTF-8</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Sql_Telegram</servlet-name>
        <servlet-class>tw.org.iii.lipin.foodsharing.Sql_Telegram</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sql_sql</servlet-name>
        <servlet-class>tw.org.iii.lipin.foodsharing.sql_sql</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sql_Inset_into</servlet-name>
        <servlet-class>tw.org.iii.lipin.foodsharing.Sql_Inset_into</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sql_delete</servlet-name>
        <servlet-class>tw.org.iii.lipin.foodsharing.Sql_delete</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sql_Telegram</servlet-name>
        <url-pattern>/Sql_Telegram</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sql_sql</servlet-name>
        <url-pattern>/sql_sql</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sql_Inset_into</servlet-name>
        <url-pattern>/Sql_Inset_into</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sql_delete</servlet-name>
        <url-pattern>/Sql_delete</url-pattern>
    </servlet-mapping>
</web-app>
